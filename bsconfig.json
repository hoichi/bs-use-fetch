{
  "name": "bs-use-fetch",
  "namespace": false,
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "bsc-flags": [ "-bs-no-version-header", "-bs-super-errors" ],
  "ppx-flags": [ "decco/ppx"],
  "bs-dependencies": [ "bs-fetch", "decco", "reason-react" ],
  "reason": {
    "react-jsx": 3
  },
  "sources": [
    {
      "dir": "src",
      "public": "all"
    },
    {
      "dir": "examples",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "suffix": ".bs.js"
}
